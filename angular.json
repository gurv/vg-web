{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ide": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/ide",
      "sourceRoot": "projects/ide/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/ide",
            "index": "projects/ide/src/index.html",
            "main": "projects/ide/src/main.ts",
            "polyfills": "projects/ide/src/polyfills.ts",
            "tsConfig": "projects/ide/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/ide/src/favicon.ico",
              "projects/ide/src/assets",
              {
                "glob": "**/*",
                "input": "assets/",
                "output": "assets/"
              },
              {
                "glob": "**/worker-*.js",
                "input": "node_modules/ace-builds/src-min/",
                "output": "assets/ace/"
              },
              {
                "glob": "javascript.js",
                "input": "node_modules/ace-builds/src-min/snippets/",
                "output": "assets/ace/snippets/"
              }
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "projects/ide/src/styles.scss",
              "node_modules/codemirror/theme/darcula.css",
              "node_modules/codemirror/lib/codemirror.css",
              "node_modules/codemirror/addon/merge/merge.css",
              "node_modules/codemirror/addon/search/matchesonscrollbar.css",
              "node_modules/codemirror/addon/dialog/dialog.css"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "styles"
              ]
            },
            "scripts": [
              "node_modules/ace-builds/src-min/ace.js",
              "node_modules/ace-builds/src-min/mode-dockerfile.js",
              "node_modules/ace-builds/src-min/mode-css.js",
              "node_modules/ace-builds/src-min/mode-html.js",
              "node_modules/ace-builds/src-min/mode-ini.js",
              "node_modules/ace-builds/src-min/mode-java.js",
              "node_modules/ace-builds/src-min/mode-javascript.js",
              "node_modules/ace-builds/src-min/mode-json.js",
              "node_modules/ace-builds/src-min/mode-less.js",
              "node_modules/ace-builds/src-min/mode-makefile.js",
              "node_modules/ace-builds/src-min/mode-markdown.js",
              "node_modules/ace-builds/src-min/mode-plain_text.js",
              "node_modules/ace-builds/src-min/mode-sass.js",
              "node_modules/ace-builds/src-min/mode-scala.js",
              "node_modules/ace-builds/src-min/mode-scss.js",
              "node_modules/ace-builds/src-min/mode-sh.js",
              "node_modules/ace-builds/src-min/mode-sql.js",
              "node_modules/ace-builds/src-min/mode-svg.js",
              "node_modules/ace-builds/src-min/mode-typescript.js",
              "node_modules/ace-builds/src-min/mode-text.js",
              "node_modules/ace-builds/src-min/mode-xml.js",
              "node_modules/ace-builds/src-min/mode-yaml.js",
              "node_modules/ace-builds/src-min/ext-searchbox.js",
              "node_modules/ace-builds/src-min/ext-language_tools.js",
              "node_modules/ace-builds/src-min/ext-modelist.js",
              "ext/mode-log.js",
              "ext/theme-vg.js",
              "node_modules/keycloak-js/dist/keycloak.js"
            ],
            "allowedCommonJsDependencies": [
              "lodash"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/ide/src/environments/environment.ts",
                  "with": "projects/ide/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ide:build",
            "proxyConfig": "projects/ide/src/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "ide:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ide:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ide/src/test.ts",
            "polyfills": "projects/ide/src/polyfills.ts",
            "tsConfig": "projects/ide/tsconfig.spec.json",
            "karmaConfig": "projects/ide/karma.conf.js",
            "assets": [
              "projects/ide/src/favicon.ico",
              "projects/ide/src/assets",
              {
                "glob": "**/*",
                "input": "assets/",
                "output": "assets/"
              }
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "projects/ide/src/styles.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "styles"
              ]
            },
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/commons/src/**/*.ts",
              "projects/commons/src/**/*.html"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/ide/e2e/protractor.conf.js",
            "devServerTarget": "ide:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ide:serve:production"
            }
          }
        }
      }
    },
    "vendors": {
      "projectType": "library",
